@{
    ViewData["Title"] = "Welcome";
}

<h2>Página Welcome trayendo datos desde el controlador</h2>

<p>Bienvenido! @ViewData["name"] con @ViewData["ID"] y edad @ViewData["age"]</p>

<!--  Agregando lógica de programación  -->
<ul>
    @for (int i = 0; i < 5; i++)
    {
            <li>@ViewData["Mensaje"]</li>
    }
</ul>